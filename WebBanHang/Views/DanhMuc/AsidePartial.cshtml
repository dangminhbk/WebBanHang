@model List<WebBanHang.Models.DanhMucSanPham>
<aside>
    <div class="header-title">
        <h3> Danh muc </h3>
    </div>
    <div class="danhmuc">
        <ul>
            @foreach( var item in Model)
            {
            <li>
                @Html.ActionLink(item.TenDanhMuc, "DanhMuc/"+item.MaDanhMuc, "DanhMucSanPhams")
            </li>
            }
        </ul>
    </div>
    <div class="filter">
        <ul class="filter-list"></ul>
    </div>
</aside>
